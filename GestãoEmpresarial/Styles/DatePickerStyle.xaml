<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region Antigo...-->
    <Style x:Key="StyleDP" TargetType="DatePicker" BasedOn="{StaticResource MaterialDesignFloatingHintDatePicker}">
     <Setter Property="FontSize" Value="15"></Setter>
     <Setter Property="Padding" Value="5"></Setter>
     
    <Setter Property="FontWeight" Value="Medium"></Setter>
        <Setter Property="FontFamily" Value="{DynamicResource CustomFont4}" />

        <Setter Property="Background" Value="{StaticResource color45}"/>
     <Setter Property="Foreground" Value="{StaticResource FontsColor1}"/>
        <Setter Property="BorderBrush" Value="{StaticResource color49}"/>
        <Setter Property="Margin" Value="5"></Setter>
 </Style>
    <Style TargetType="DatePicker" BasedOn="{StaticResource StyleDP}">
 </Style>
    <!--#endregion-->
</ResourceDictionary>