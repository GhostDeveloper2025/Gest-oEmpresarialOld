    <!--  ToggleButton Pesquisar  -->
    <Style x:Key="ToggleButtonPesquisarStyle"
           BasedOn="{StaticResource MaterialDesignActionToggleButton}"
           TargetType="ToggleButton">
        <Setter Property="Foreground" Value="{DynamicResource PrimaryButtonPesquisarColor}" />
