                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes">
    <!--  Botao Salvar  -->
    <Style x:Key="ButonSalvarStyle"
           TargetType="Button">
        <Setter Property="Height" Value="50" />
        <Setter Property="Width" Value="160" />
        <Setter Property="Margin" Value="5,0,0,5" />
        <Setter Property="Background" Value="{DynamicResource PrimaryBackgroundButtonColor}" />
        <Setter Property="Foreground" Value="{DynamicResource PrimaryTextButtonColor}" />
        <Setter Property="BorderBrush" Value="{DynamicResource SecundaryTransparenteColor}" />
        <Setter Property="ToolTip" Value="Clique Para Salvar!" />
        <Setter Property="FontFamily" Value="{DynamicResource CustomFont4}" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Left" />

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            CornerRadius="5">
                        <Grid>
                            <!--  Define duas colunas no Grid  -->
                            <Grid.ColumnDefinitions>
                                <!--  Largura para o ícone  -->
                                <ColumnDefinition Width="50" />
                                <!--  Espaço restante para o texto  -->
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <!--  Ícone na primeira coluna  -->
                            <materialDesign:PackIcon Grid.Column="0"
                                                     Width="40"
                                                     Height="40"
                                                     Margin="5,0,5,0"
                                                     HorizontalAlignment="Left"
                                                     VerticalAlignment="Center"
                                                     Kind="ContentSaveCheck" />

                            <!--  Texto na segunda coluna  -->
                            <TextBlock Grid.Column="1"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="{TemplateBinding FontFamily}"
                                       FontSize="16"
                                       Foreground="{TemplateBinding Foreground}"
                                       Text="Salvar" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--  Trigger para mudar a cor ao passar o mouse  -->
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <!--  Muda A Cor Do Fundo do Botao Ao Passar O Mouse  -->
                <Setter Property="Background" Value="{DynamicResource PrimaryOverBackgroundButtonColor}" />
                <!--  Muda A Cor Da Fonte AO Passar O Mouse  -->
                <Setter Property="Foreground" Value="{DynamicResource PrimaryOverTextButtonColor}" />
            </Trigger>
        </Style.Triggers>
    </Style>

