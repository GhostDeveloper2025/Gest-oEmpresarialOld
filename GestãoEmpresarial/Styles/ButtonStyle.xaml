        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="CheckBox">
                    <StackPanel Orientation="Horizontal">
                        <Border Width="16"
                                Height="16"
                                BorderBrush="{DynamicResource PrimaryBorderColor}"
                                BorderThickness="2">
                            <Grid>
                                <Rectangle Width="16"
                                           Height="16"
                                           Fill="{DynamicResource SecundaryTransparenteColor}" />
