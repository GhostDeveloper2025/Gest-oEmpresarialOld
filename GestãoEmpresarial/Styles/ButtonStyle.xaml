                <materialDesign:PackIcon Width="30"
                                         Height="30"
                                         Kind="ArrowRightBoldCircleOutline" />
            </Setter.Value>
        </Setter>
        <Setter Property="materialDesign:ToggleButtonAssist.OnContent">
            <Setter.Value>
                <materialDesign:PackIcon Width="30"
                                         Height="30"
                                         Kind="CheckboxMarkedCircleOutline" />
