<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit">
    <!--#region Antigo...-->
    <Style x:Key="StyleTB"
           BasedOn="{StaticResource MaterialDesignFloatingHintTextBox}"
           TargetType="TextBox">
        <Setter Property="FontSize" Value="15" />
        <Setter Property="Padding" Value="5" />
        <!--<Setter Property="FontWeight" Value="Medium"></Setter>-->
        <Setter Property="FontFamily" Value="{DynamicResource CustomFont4}" />
        <!--<Setter Property="CharacterCasing" Value="Upper"></Setter>-->
        <Setter Property="Background" Value="{DynamicResource SecundaryTransparenteColor}" />
        <Setter Property="Foreground" Value="{DynamicResource PrimaryTextTextboxColor}" />
        <Setter Property="BorderBrush" Value="{DynamicResource PrimaryBorderTextboxColor}" />
        <!--<Setter Property="CaretBrush" Value="{DynamicResource SecundaryColor6}" />-->
        <Setter Property="Margin" Value="5" />
    </Style>
    <Style BasedOn="{StaticResource StyleTB}"
           TargetType="TextBox" />
</ResourceDictionary>