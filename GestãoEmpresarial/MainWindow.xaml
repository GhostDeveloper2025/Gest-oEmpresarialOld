<Window x:Class="GestãoEmpresarial.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Background="{DynamicResource color37 }"
        TextElement.FontWeight="Medium"
        TextElement.FontSize="18"
        FontFamily="{DynamicResource CustomFont4}"
        xmlns:local="clr-namespace:GestãoEmpresarial"
        xmlns:layout="clr-namespace:GestãoEmpresarial.Views.Layout"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <StackPanel Background="{StaticResource color59}" Width="250">
            <!--Painel De Informações:-->
            <TextBlock Text="VERSÃO DO SOFTWARE"/>
            <TextBlock Text="V1.0"/>
            <layout:MenuView ></layout:MenuView>
        </StackPanel>
        <Border DockPanel.Dock="Bottom"  Margin="10,10,10,10" Background="{StaticResource color59}" CornerRadius="10">
            <Grid  DockPanel.Dock="Bottom" Name="fContainer" Margin="10,10,10,10" Background="{StaticResource color59}" />
        </Border>
    </DockPanel>
</Window>
